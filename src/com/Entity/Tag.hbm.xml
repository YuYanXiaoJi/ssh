<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping  >

    <class name="com.Entity.Tag" table="tb_Tag" schema="dbo" catalog="City">
       
        <id name="id" type="java.lang.Integer" column="id">
            <generator class="native"/>
        </id>
        
        <!-- 必须唯一 -->
        <property name="tag" type="java.lang.String" unique="true"  ></property>
       	
       	<property name="tagCount" type="java.lang.Integer"></property>
       	
    </class>
    

</hibernate-mapping>
